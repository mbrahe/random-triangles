pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
function midpoint(p1,p2)
	-- input: two points
	-- output: point at the center of the line segment connecting both points

	local x = (p1.x + p2.x) / 2
	local y = (p1.y + p2.y) / 2

	return {x=x, y=y}
end

function length(point1, point2)
	-- Input: two points
	-- Output: length of the two points

	local xdiff = point1.x - point2.x
	local ydiff = point1.y - point2.y

	return sqrt(xdiff^2 + ydiff^2)
end


function triangle (points, fillcol, side)
	-- input: table of 3 points a color, and an int indicating which side, 0 or nil indicates all sides
	-- draws a filled in triangle

	if (not side or side == 1) then
		line(points[1].x, points[1].y, points[2].x, points[2].y, fillcol)
	end
	if (not side or side == 2) then
		line(points[2].x, points[2].y, points[3].x, points[3].y, fillcol)
	end
	if (not side or side == 3) then
		line(points[1].x, points[1].y, points[3].x, points[3].y, fillcol)
	end

	if (length(points[1], points[2]) > 1 or 
		length(points[2], points[3]) > 1 or 
		length(points[1], points[3]) > 1) then

		local midpoints = {}
		add(midpoints, midpoint(points[1], points[2]))
		add(midpoints, midpoint(points[2], points[3]))
		add(midpoints, midpoint(points[3], points[1]))

		triangle({points[1], midpoints[1], midpoints[3]}, fillcol, 2)
		triangle({points[2], midpoints[1], midpoints[2]}, fillcol, 2)
		triangle({points[3], midpoints[2], midpoints[3]}, fillcol, 2)
		triangle({midpoints[1], midpoints[2], midpoints[3]}, fillcol)
	end
end

function draw_triangles(n) 
	-- draws n randomly colored triangles in random locations
	cls()

	for i=1,n do

		local points = {}
		add(points, {x=flr(rnd(128)), y=flr(rnd(128))})
		add(points, {x=flr(rnd(128)), y=flr(rnd(128))})
		add(points, {x=flr(rnd(128)), y=flr(rnd(128))})

		triangle(points, flr(rnd(15)) + 1)
	end
end

function _init()
	draw_triangles(20)
end

function _update()
	if (btn(4)) then
		draw_triangles(20)
	end
end

